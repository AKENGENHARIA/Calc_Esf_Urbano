{
    "hosting": {
      "public": ".",  // Isso aponta para o diretório atual, onde seu index.html está
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  }
  